<h1>Registrations</h1>

  <a href="/registrations/new">New registration!</a>

<ul>
  <% for registration in @registrations %>
  <li>
  <p><%= registration.member.first_name %> <%= registration.member.last_name %>
    <%= registration.activity.class_name %> </p>
    <p>
      <form action="/registrations/<%= registration.id %>/delete" method="POST">
        <input type="submit" value="delete" />
      </form>
    </p>
  </li>
  <% end %>
</ul>
